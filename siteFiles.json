---
layout: null
---

{
  "pages": [
    {% for page in site.pages %}"{{ page.url }}"{% unless forloop.last %},{% endunless %}
    {% endfor -%}
  ],
  "assets": [
    {% for item in site.static_files -%}
    {% unless item.path contains '/assets/scss/' or item.path contains '/assets/js/src/' -%}
    "{{ item.path }}"{% unless forloop.last %},{% endunless %}
    {% endunless -%}
  {%- endfor -%}
  ]
}